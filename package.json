{
  "name": "ss1e",
  "version": "1.0.0",
  "description": "CSS compiler for the ss1e system",
  "scripts": {
    "build": "sass src/scss/ss1e.scss css/ss1e.css --style=expanded --no-source-map",
    "watch": "sass src/scss/ss1e.scss css/ss1e.css --style=expanded --source-map --watch",
    "generate": "node src/generate-ss1e-system.mjs"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "author": "Asacolips",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "glob": "^10.3.10",
    "inquirer": "^9.2.15",
    "prompt": "^1.3.0",
    "renamer": "^5.0.0",
    "replace": "^1.2.2",
    "sass": "^1.53.0"
  },
  "dependencies": {
    "handlebars": "^4.7.8",
    "less": "^4.2.0"
  }
}
